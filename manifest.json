{
    "name": "chrome-history-sync",
    "description": "",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
        "history"
    ],
    "host_permissions": [
        "http://127.0.0.1:9200/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "/images/icon16.png",
            "32": "/images/icon32.png",
            "48": "/images/icon48.png",
            "128": "/images/icon128.png"
        }
    },
    "icons": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "options_page": "options.html"
}
